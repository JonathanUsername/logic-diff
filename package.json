{
  "name": "ts-safe-diff",
  "version": "1.0.0",
  "description": "Ensure no logic changes in git diff",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node -T ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "author": "Jonathan King",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "diff-trees": "^0.4.2",
    "jscodeshift": "^0.13.1",
    "lodash": "^4.17.21",
    "pkg": "^5.7.0",
    "rimraf": "^3.0.2",
    "simple-git": "^3.7.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.11.5",
    "@types/node": "^17.0.38",
    "jscodeshift-debug": "^1.1.1"
  }
}
