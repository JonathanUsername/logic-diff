{
  "name": "ts-safe-diff",
  "version": "1.0.0",
  "description": "Ensure no logic changes in git diff",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "author": "Jonathan King",
  "license": "ISC",
  "dependencies": {
    "jscodeshift": "^0.13.1",
    "pkg": "^5.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.11.5"
  }
}
